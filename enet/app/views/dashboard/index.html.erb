<body class="dashboard">


  <div class="grid" style="padding: 20px">

    <% if @modules.length > 0 %>
      <h3 >Modules</h3>
      <div id="modules-list" class="row" style="padding-top: 10px">
        <% @modules.each do |university_module, index| %>

          <div class=" col-md-3 " style="">
            <div class="card module_card" style="height: 100px">
              <label style="font-size: 24px; color:#0e0e0e; text-align: center; padding-top: 10px; margin-left: 5px; margin-right: 5px"> <%= university_module.name %></label>
            </div>

          </div>

        <% end %>
      </div>
      <% end %>


    <div style="padding-top: 30px"></div>

    <h3>Other Utilities</h3>
    <div id="modules-list" class="row" style="padding-top: 10px">

      <div class=" col-md-3 " style="">
        <div class="card module_card" style="height: 100px">
          <label style="font-size: 24px; color:#0e0e0e; text-align: center; padding-top: 10px"> Live Chat</label>
        </div>

      </div>
    </div>


  </div>

